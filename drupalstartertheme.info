name = drupalstartertheme
description = A Drupal 7 blank starter theme for use as a starting point for building custom themes.
version = 2.0
core = 7.x

; list stylesheets
stylesheets[all][] = assets/css/styles.css
stylesheets[print][] = assets/css/print.css

; list scripts
scripts[] = assets/js/init.js

regions[header] = Header
regions[help] = Help
regions[page_top] = Page top
regions[page_bottom] = Page bottom
regions[highlighted] = Highlighted

regions[featured] = Featured
regions[content] = Content
regions[sidebar_first] = Sidebar first
regions[sidebar_second] = Sidebar second

regions[footer] = Footer
